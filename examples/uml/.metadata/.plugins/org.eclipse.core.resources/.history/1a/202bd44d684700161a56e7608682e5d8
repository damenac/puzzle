	bind( ComponentsRequired, ClassesProvided )
	bind( ComponentsRequired, CommonBehaviorsProvided )
	bind( CompositeStructuresRequired, ClassesProvided )
	bind( CompositeStructuresRequired, ComponentsProvided )
	bind( CompositeStructuresRequired, CommonBehaviorsProvided )
	bind( CommonBehaviorsRequired, ClassesProvided )
	bind( DeploymentsRequired, ClassesProvided )
	bind( ActionsRequired, ClassesProvided )
	bind( ActionsRequired, CommonBehaviorsProvided )
	bind( ActivitiesRequired, ClassesProvided )
	bind( ActivitiesRequired, CommonBehaviorsProvided )
	bind( ActivitiesRequired, ActionsProvided )
	bind( InteractionsRequired, ClassesProvided )
	bind( InteractionsRequired, ComponentsProvided )
	bind( InteractionsRequired, CommonBehaviorsProvided )
	bind( StateMachinesRequired, ClassesProvided )
	bind( StateMachinesRequired, CommonBehaviorsProvided )
	bind( StateMachinesRequired, CompositeStructuresProvided )
	bind( UseCasesRequired, ClassesProvided )
}