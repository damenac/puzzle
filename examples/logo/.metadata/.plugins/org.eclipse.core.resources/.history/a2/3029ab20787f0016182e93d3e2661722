	bind( RequiredPrimitives, ProvidedLiterals )
	bind( RequiredBinaryExpressions, ProvidedLiterals )
	bind( RequiredVariables, ProvidedLiterals )
	bind( RequiredVariables, ProvidedPrimitives )
	bind( RequiredControlStructures, ProvidedLiterals )
	bind( RequiredControlStructures, ProvidedPrimitives )
	bind( RequiredControlStructures, ProvidedLiterals )
	bind( RequiredControlStructures, ProvidedPrimitives )
}