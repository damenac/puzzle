<?xml version="1.0" encoding="ASCII"?>
<fsm:StateMachine
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:fsm="http://puzzle.examples.fsm"
    xsi:schemaLocation="http://puzzle.examples.fsm ../../fr.inria.diverse.puzzle.examples.fsm.syntax/models/fsm.ecore"
    name="TheCitizenWatch">
  <regions name="MainRegion">
    <subvertex
        xsi:type="fsm:InitialState"
        name="Initial"
        outgoing="//@regions.0/@transitions.0"/>
    <subvertex
        xsi:type="fsm:State"
        name="Displays"
        incoming="//@regions.0/@transitions.0"
        outgoing="//@regions.0/@transitions.1 //@regions.0/@transitions.2"/>
    <subvertex
        xsi:type="fsm:State"
        name="Alarm 1"
        incoming="//@regions.0/@transitions.1"/>
    <subvertex
        xsi:type="fsm:State"
        name="Alarm 2"
        incoming="//@regions.0/@transitions.2"/>
    <transitions
        name="start"
        target="//@regions.0/@subvertex.1"
        source="//@regions.0/@subvertex.0"/>
    <transitions
        name="T1"
        target="//@regions.0/@subvertex.2"
        source="//@regions.0/@subvertex.1">
      <trigger
          expression="T1"/>
    </transitions>
    <transitions
        name="T2"
        target="//@regions.0/@subvertex.3"
        source="//@regions.0/@subvertex.1">
      <trigger
          expression="T2"/>
    </transitions>
  </regions>
</fsm:StateMachine>
