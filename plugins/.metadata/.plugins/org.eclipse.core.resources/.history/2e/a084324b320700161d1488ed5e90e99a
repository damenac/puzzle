package fr.inria.diverse.puzzle.metrics.vos;

import org.eclipse.emf.ecore.EClass;

public class HCTreeNode {

	// ----------------------------------------------
	// Attributes
	// ----------------------------------------------
	
	private EClass eClass;

	// ----------------------------------------------
	// Constructor
	// ----------------------------------------------
	
	public HCTreeNode(){}

	// ----------------------------------------------
	// Methods
	// ----------------------------------------------

	public EClass geteClass() {
		return eClass;
	}

	public void seteClass(EClass eClass) {
		this.eClass = eClass;
	}
}
