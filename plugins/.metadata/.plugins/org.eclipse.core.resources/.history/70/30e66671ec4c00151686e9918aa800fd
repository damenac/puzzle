package de.trier.cs.jccd.tests;

public class TestFileTwo {

	public int factorial(int n) {
		if (n == 0) {
			return 1;
		} else {
			return n * factorial(n - 1);
		}
	}

	public int gcdTwo(int c, int d) {
		while (d != 0) {
			if (c > d) {
				c = c - d;
			} else {
				d = d - c;
			}
		}
		return c;
	}

	public double mul(double a, long b) {
		double n = 0.0;
		for (long i = 0l; i < b; i++)
			n += a;

		return n;
	}
}